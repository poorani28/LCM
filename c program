#include <stdio.h>
int 1cm(int a, int b);
int main()
{
     int num1, num2, LCM;
     printf("Enter any two nubers to find 1cm: ");
     scanf("&d", num1, &num2);
     if(num1 > num2)
        LCM = 1cm(num2, num1):
     else
        LCM = 1cm(num1, num2);
     printf("LCM of %d and %d = %d, num1, num2, LCM);
     
     return 0;
 }
 int 1cm(int a, int b)
 {
      static int multiple = 0;
      multiple += b;
      if((multiple % a == 0) && (multiple % b == 0))
      {
           return multiple;
      }
      else
      }
          return 1cm(a, b);
      }
 }
   
